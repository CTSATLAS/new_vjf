        %dt Veteran Resources
        %dd
          %ul
            %li= link_to 'Continuing Education Resources for Veterans', 'http://www.google.com/url?q=http%3A%2F%2Fwww.accreditedonlinecolleges.org%2Fresources%2Fveteran-continuing-ed%2F&sa=D&sntz=1&usg=AFQjCNHk2x88i1kCbWoMpXkkNzvdh79ipg', target: '_blank'
